package com.mycompany.objekgeometri;

/**
 *
 * @author ACER
 */
public class JajarGenjang extends ObjekGeometri {
     private double alas, tinggi;
    
    JajarGenjang(double alas, double tinggi){
        this.alas = alas;
        this.tinggi = tinggi;
    }
    

    public String getLuas(){
        return "Luas Jajar genjang = "+0.5 * alas * tinggi;
    }
    
}
